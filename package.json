{
  "name": "promeet",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "stylelint": "stylelint \"src/**/*.{.css,js,jsx}\"",
